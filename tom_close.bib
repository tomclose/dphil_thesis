@article{Calderbank:1996p3959,
	Abstract = {A quantum error-correcting code is defined to be a unitary mapping (encoding) of k qubits (two-state quantum systems) into a subspace of the quantum state space of n qubits such that if any t of the qubits undergo arbitrary decoherence, not necessarily independently, the resulting n qubits can be used to faithfully reconstruct the original quantum state of the k encoded qubits. Quantum error-correcting codes are shown to exist with asymptotic rate k/n=1-2H(2)(2t/n) where H-2(p) is the binary entropy function -plog(2)p-(1-p)log(2)(1-p). Upper bounds on this asymptotic rate are given.},
	Author = {AR Calderbank and PW Shor},
	Date-Added = {2013-03-23 17:03:39 +0000},
	Date-Modified = {2013-03-23 17:03:39 +0000},
	Journal = {Physical Review A},

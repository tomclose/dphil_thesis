@article{neumann10,
	Author = {Neumann, Philipp and Beck, Johannes and Steiner, Matthias and Rempp, Florian and Fedder, Helmut and Hemmer, Philip R. and Wrachtrup, J{\"o}rg and Jelezko, Fedor},
	Date = {2010/07/30},

	Journal = {Annalen der Physik},
	Keywords = {1D, Electron-Gas, Tomonaga-Luttinger Liquids, Luttinger Liquids, Fermi-Edge Singularity, Bosonization, Enhancement, Dimensional Quantum Fluids, Model, Points, Refermionization, Density-Of-States, Sine-Gordon Equation},
	Language = {English},

@article{quantum_chem_alg_05,
	Author = {Aspuru-Guzik, Al{\'a}n and Dutoi, Anthony D. and Love, Peter J. and Head-Gordon, Martin},
	Date = {2005/09/09},
	Date-Added = {2013-04-04 17:11:12 +0100},
	Date-Modified = {2013-04-04 17:13:18 +0100},
	Journal = {Science},
	M3 = {10.1126/science.1113479},
	Month = {09},
	Number = {5741},
	Pages = {1704--1707},
	Title = {Simulated Quantum Computation of Molecular Energies},
	Ty = {JOUR},
	Url = {http://www.sciencemag.org/content/309/5741/1704.abstract N2 - The calculation time for the energy of atoms and molecules scales exponentially with system size on a classical computer but polynomially using quantum algorithms. We demonstrate that such algorithms can be applied to problems of chemical interest using modest numbers of quantum bits. Calculations of the water and lithium hydride molecular ground-state energies have been carried out on a quantum computer simulator using a recursive phase-estimation algorithm. The recursive algorithm reduces the number of quantum bits required for the readout register from about 20 to 4. Mappings of the molecular wave function to the quantum bits are described. An adiabatic method for the preparation of a good approximate ground-state wave function is described and demonstrated for a stretched hydrogen molecule. The number of quantum bits required scales linearly with the number of basis functions, and the number of gates required grows polynomially with the number of quantum bits.},
	Volume = {309},
	Year = {2005},
	Bdsk-Url-1 = {http://www.sciencemag.org/content/309/5741/1704.abstract%20N2%20-%20The%20calculation%20time%20for%20the%20energy%20of%20atoms%20and%20molecules%20scales%20exponentially%20with%20system%20size%20on%20a%20classical%20computer%20but%20polynomially%20using%20quantum%20algorithms.%20We%20demonstrate%20that%20such%20algorithms%20can%20be%20applied%20to%20problems%20of%20chemical%20interest%20using%20modest%20numbers%20of%20quantum%20bits.%20Calculations%20of%20the%20water%20and%20lithium%20hydride%20molecular%20ground-state%20energies%20have%20been%20carried%20out%20on%20a%20quantum%20computer%20simulator%20using%20a%20recursive%20phase-estimation%20algorithm.%20The%20recursive%20algorithm%20reduces%20the%20number%20of%20quantum%20bits%20required%20for%20the%20readout%20register%20from%20about%2020%20to%204.%20Mappings%20of%20the%20molecular%20wave%20function%20to%20the%20quantum%20bits%20are%20described.%20An%20adiabatic%20method%20for%20the%20preparation%20of%20a%20good%20approximate%20ground-state%20wave%20function%20is%20described%20and%20demonstrated%20for%20a%20stretched%20hydrogen%20molecule.%20The%20number%20of%20quantum%20bits%20required%20scales%20linearly%20with%20the%20number%20of%20basis%20functions,%20and%20the%20number%20of%20gates%20required%20grows%20polynomially%20with%20the%20number%20of%20quantum%20bits.}}

